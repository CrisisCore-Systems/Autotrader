[run]
branch = True
source = autotrader
omit =
    */__init__.py
    */tests/*
    */test_*.py
    setup.py
    
[report]
fail_under = 85
show_missing = True
skip_covered = False
precision = 2

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
