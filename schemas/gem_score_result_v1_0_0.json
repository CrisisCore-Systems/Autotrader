{
  "schema_id": "gem_score_result",
  "schema_type": "artifact",
  "version": "1.0.0",
  "created_at": "2025-10-09T00:00:00Z",
  "description": "Output schema for gem score calculation results",
  "backward_compatible": true,
  "breaking_changes": [],
  "fields": [
    {
      "name": "score",
      "type": "float",
      "required": true,
      "nullable": false,
      "description": "Final gem score (0-100 scale)",
      "constraints": {
        "min": 0,
        "max": 100
      },
      "added_in_version": "1.0.0"
    },
    {
      "name": "token_address",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "Contract address of the token",
      "constraints": {
        "pattern": "^0x[a-fA-F0-9]{40}$"
      },
      "added_in_version": "1.0.0"
    },
    {
      "name": "token_symbol",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "Token trading symbol",
      "constraints": {
        "min_length": 1,
        "max_length": 20
      },
      "added_in_version": "1.0.0"
    },
    {
      "name": "calculated_at",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "ISO 8601 timestamp of calculation",
      "added_in_version": "1.0.0"
    },
    {
      "name": "confidence",
      "type": "float",
      "required": true,
      "nullable": false,
      "description": "Confidence level (0-1 scale)",
      "constraints": {
        "min": 0,
        "max": 1
      },
      "added_in_version": "1.0.0"
    },
    {
      "name": "breakdown",
      "type": "object",
      "required": true,
      "nullable": false,
      "description": "Score breakdown by component",
      "added_in_version": "1.0.0"
    },
    {
      "name": "flags",
      "type": "array",
      "required": false,
      "nullable": false,
      "description": "Warning or alert flags",
      "default": [],
      "added_in_version": "1.0.0"
    },
    {
      "name": "metadata",
      "type": "object",
      "required": false,
      "nullable": false,
      "description": "Additional metadata",
      "default": {},
      "added_in_version": "1.0.0"
    }
  ],
  "examples": [
    {
      "score": 87.5,
      "token_address": "0x1234567890123456789012345678901234567890",
      "token_symbol": "GEM",
      "calculated_at": "2025-10-09T12:00:00Z",
      "confidence": 0.92,
      "breakdown": {
        "liquidity": 85.0,
        "volume": 90.0,
        "holders": 88.0,
        "safety": 95.0
      },
      "flags": [],
      "metadata": {
        "chain": "ethereum",
        "dex": "uniswap"
      }
    }
  ],
  "metadata": {
    "owner": "autotrader-core",
    "category": "scoring"
  }
}
