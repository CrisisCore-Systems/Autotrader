{
  "schema_id": "market_snapshot",
  "schema_type": "artifact",
  "version": "1.0.0",
  "created_at": "2025-10-09T00:00:00Z",
  "description": "Market snapshot data structure",
  "backward_compatible": true,
  "breaking_changes": [],
  "fields": [
    {
      "name": "token_address",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "Contract address",
      "constraints": {
        "pattern": "^0x[a-fA-F0-9]{40}$"
      }
    },
    {
      "name": "symbol",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "Token symbol"
    },
    {
      "name": "price_usd",
      "type": "float",
      "required": true,
      "nullable": false,
      "description": "Current price in USD",
      "constraints": {
        "min": 0
      }
    },
    {
      "name": "liquidity_usd",
      "type": "float",
      "required": true,
      "nullable": false,
      "description": "Total liquidity in USD",
      "constraints": {
        "min": 0
      }
    },
    {
      "name": "volume_24h_usd",
      "type": "float",
      "required": true,
      "nullable": false,
      "description": "24-hour trading volume in USD",
      "constraints": {
        "min": 0
      }
    },
    {
      "name": "market_cap_usd",
      "type": "float",
      "required": false,
      "nullable": true,
      "description": "Market capitalization in USD"
    },
    {
      "name": "holder_count",
      "type": "int",
      "required": false,
      "nullable": true,
      "description": "Number of token holders",
      "constraints": {
        "min": 0
      }
    },
    {
      "name": "price_change_24h",
      "type": "float",
      "required": false,
      "nullable": true,
      "description": "24-hour price change percentage"
    },
    {
      "name": "snapshot_timestamp",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "ISO 8601 timestamp of snapshot"
    },
    {
      "name": "source",
      "type": "string",
      "required": true,
      "nullable": false,
      "description": "Data source identifier",
      "constraints": {
        "enum": ["dexscreener", "etherscan", "manual", "test"]
      }
    }
  ],
  "examples": [
    {
      "token_address": "0x1234567890123456789012345678901234567890",
      "symbol": "GEM",
      "price_usd": 0.00123,
      "liquidity_usd": 50000.0,
      "volume_24h_usd": 12000.0,
      "market_cap_usd": 1000000.0,
      "holder_count": 1500,
      "price_change_24h": 15.5,
      "snapshot_timestamp": "2025-10-09T12:00:00Z",
      "source": "dexscreener"
    }
  ]
}
